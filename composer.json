    "require": {
        "php"                           : ">=5.4",
        "symfony/http-foundation"       : "~2.3",
        "symfony/form"                  : "~2.3"
    },

    "require-dev": {
        "phpunit/phpunit": "4.*"
    },

    "minimum-stability": "stable",

    "autoload": {
        "psr-4" : {
            "Mitom\\Bundle\\FormHandlerBundle\\" : "src/"
        }
    },

    "autoload-dev": {
        "psr-4" : {
            "Mitom\\Bundle\\FormHandlerBundle\\" : "test/"
        }
    }
}